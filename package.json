{
  "name": "badger-badger",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel serve src/index.html --public-url '/badger-badger/' --no-cache --dist-dir dist/coverator",
