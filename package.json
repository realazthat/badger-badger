    "build": "parcel build src/index.html --public-url '/badger-badger/' --no-cache --dist-dir dist/coverator",
    "format:js": "semistandard --fix --ignore 'dist/*'",
    "format:md": "prettier --write **/*.md",
    "format": "npm run format:js && npm run format:md",
    "deploy": "gh-pages -d dist/coverator -b gh-pages",
    "flow": "flow",
    "flow:stop": "flow stop",
    "flow:check": "flow check"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-flow-strip-types": "^7.23.3",
    "@parcel/babel-plugin-transform-runtime": "^2.11.0",
    "flow-bin": "^0.228.0",
    "gh-pages": "^6.1.1",
    "parcel": "^2.11.0",
    "prettier": "^3.2.5",
    "semistandard": "^17.0.0"
  },
  "dependencies": {
    "serve": "^14.2.1"
  },
  "babel": {
    "presets": [
      "@parcel/babel-preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@parcel/babel-plugin-transform-runtime"
    ]
  }
